{
	"name": "lisk-sdk-scripts",
	"version": "1.0.0",
	"description": "Some tests scripts developed over lisk-sdk",
	"private": true,
	"scripts": {
		"test": "./node_modules/.bin/jest --runInBand  --verbose",
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*'"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/nazarhussain/lisdk-sdk-scripts.git"
	},
	"author": "Nazar Hussain",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/nazarhussain/lisdk-sdk-scripts/issues"
	},
	"homepage": "https://github.com/nazarhussain/lisdk-sdk-scripts#readme",
	"dependencies": {
		"@liskhq/lisk-chain": "0.2.0-alpha.0",
		"@liskhq/lisk-codec": "0.1.0-alpha.0",
		"@liskhq/lisk-cryptography": "3.0.0-alpha.0",
		"@liskhq/lisk-genesis": "0.1.0-alpha.0",
		"@liskhq/lisk-transactions": "5.0.0-alpha.0",
		"@liskhq/lisk-utils": "0.1.0-alpha.0",
		"cucumber": "6.0.5",
		"jest": "26.4.2",
		"jest-extended": "0.11.5",
		"lisk-forger-api-client": "1.0.0",
		"lisk-http-api-client": "1.0.0",
		"lisk-sdk": "5.0.0-alpha.0"
	},
	"engines": {
		"node": ">=12.13.0 <=12",
		"npm": ">=6.12.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"workspaces": {
		"packages": [
			"api_clients/*"
		],
		"nohoist": []
	},
	"devDependencies": {
		"@types/cucumber": "6.0.1",
		"@types/expect": "24.3.0",
		"@types/node": "14.6.2",
		"@typescript-eslint/eslint-plugin": "3.5.0",
		"@typescript-eslint/parser": "3.5.0",
		"eslint": "7.3.1",
		"eslint-config-airbnb-base": "14.2.0",
		"eslint-config-lisk-base": "1.2.2",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-chai-expect": "2.1.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jest": "23.17.1",
		"eslint-plugin-mocha": "7.0.1",
		"husky": "4.2.5",
		"lint-staged": "10.2.11",
		"prettier": "2.1.1",
		"ts-jest": "26.3.0",
		"ts-node": "8.6.2",
		"typescript": "4.0.2",
		"yarn": "1.22.4"
	}
}
